{
  "name": "monad-ecosystem-explorer",
  "version": "1.0.0",
  "description": "A comprehensive React application for exploring the Monad blockchain ecosystem with AI chatbot and demo swap interface",
  "author": "sugarufc <sugarufc@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "docker-compose up --build",
    "stop": "docker-compose down",
    "lint": "eslint src/**/*.{js,jsx}",
    "format": "prettier --write src/**/*.{js,jsx,css}",
    "deploy": "./deploy-vercel.sh",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web3": "^4.0.1",
    "ethers": "^6.7.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^4.9.5",
    "eslint": "^8.0.0",
    "prettier": "^2.8.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sugarufc/monad-ecosystem-explorer.git"
  },
  "keywords": [
    "monad",
    "blockchain",
    "react",
    "ethereum-compatible",
    "defi",
    "ai-chatbot",
    "docker",
    "security",
    "javascript",
    "web3"
  ]
} 